package com.econsys.UIobjectrepositary;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class PAGEstimationDesignPageObjects {

	@FindBy(xpath="//input[@id='submitEstimation']")
	private WebElement submit_for_Estimation;

	@FindBy(xpath="//*[@id='st_comments_estimation']")
	private WebElement estimation_Comments;
	
	@FindBy(xpath="//input[@id='submitDesignButton']")
	private WebElement submit_for_Design;
	
	@FindBy(xpath="//div[@id='estimationCalculationDocs-dropzone']")
	private WebElement estimation_dropzone;
	
	@FindBy(xpath="//div[@id='designDocs-dropzone']")
	private WebElement design_Dropzone;
	
	@FindBy(xpath="//*[@id='st_comments_design']")
	private WebElement design_Comments;
	
	
	public WebElement getSubmit_for_Estimation() {
		return submit_for_Estimation;
	}

	public WebElement getSubmit_for_Design() {
		return submit_for_Design;
	}

	public WebElement getEstimation_Comments() {
		return estimation_Comments;
	}

	public WebElement getEstimation_dropzone() {
		return estimation_dropzone;
	}

	public WebElement getDesign_Dropzone() {
		return design_Dropzone;
	}

	public WebElement getDesign_Comments() {
		return design_Comments;
	}
}
